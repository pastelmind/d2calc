{
  "name": "d2calc",
  "version": "0.1.1",
  "description": "Interpreter for the mini-language used internally by Diablo 2",
  "type": "module",
  "main": "./dist/d2calc.umd.js",
  "module": "index.js",
  "exports": {
    "import": "./index.js",
    "require": "./dist/d2calc.umd.cjs"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rollup --config && tsc -p jsconfig.build.json",
    "clean": "rm -rf ./dist/*",
    "lint": "eslint . && tsc -p jsconfig.json",
    "prepare": "npm run build",
    "test": "mocha --experimental-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pastelmind/d2calc.git"
  },
  "keywords": [
    "diablo",
    "diablo-2",
    "interpreter"
  ],
  "author": "Yehyoung Kang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pastelmind/d2calc/issues"
  },
  "homepage": "https://github.com/pastelmind/d2calc#readme",
  "files": [
    "dist/",
    "src/"
  ],
  "devDependencies": {
    "@types/mocha": "^8.0.2",
    "@types/node": "^12.12.54",
    "@types/sinon": "^9.0.4",
    "eslint": "^7.6.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.1.1",
    "prettier": "^2.0.5",
    "rollup": "^2.24.0",
    "rollup-plugin-terser": "^7.0.0",
    "sinon": "^9.0.3",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=10"
  }
}
